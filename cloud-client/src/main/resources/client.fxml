      <Button fx:id="newFolderServer" onMouseClicked="#NewFolderOnServer" layoutX="368.0" layoutY="414.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="106.0" text="New folder" AnchorPane.bottomAnchor="56.0" AnchorPane.rightAnchor="126.0" />
      <Button fx:id="deleteServer" onMouseClicked="#deleteFromServer" layoutX="474.0" layoutY="414.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="106.0" text="Delete" AnchorPane.bottomAnchor="56.0" AnchorPane.rightAnchor="10.0" />
      <Button fx:id="refreshServer" layoutX="10.0" layoutY="450.0" mnemonicParsing="false" onMouseClicked="#refreshServerListView" prefHeight="36.0" prefWidth="222.0" text="Refresh" AnchorPane.bottomAnchor="10.0" AnchorPane.rightAnchor="10.0" />

   </children>
