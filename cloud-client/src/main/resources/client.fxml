<AnchorPane prefHeight="500.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientController">
   <children>
      <Button fx:id="requestButton" layoutX="244.0" layoutY="196.0" mnemonicParsing="false" onMouseClicked="#requestFile" prefHeight="80.0" prefWidth="50.0" text="&lt;" AnchorPane.leftAnchor="245.0" AnchorPane.topAnchor="200.0" />
      <Button fx:id="sendButton" layoutX="316.0" layoutY="166.0" mnemonicParsing="false" onMouseClicked="#sendFile" prefHeight="80.0" prefWidth="50.0" text="&gt;" AnchorPane.rightAnchor="245.0" AnchorPane.topAnchor="200.0" />

      <Label layoutX="94.0" layoutY="14.0" text="Computer" />
      <ListView fx:id="clientListView" layoutX="14.0" layoutY="14.0" prefHeight="412.0" prefWidth="222.0" AnchorPane.bottomAnchor="102.0" AnchorPane.leftAnchor="10.0" AnchorPane.topAnchor="40.0" />
      <Button fx:id="newFolderClient" onMouseClicked="#newFolderOnClient" layoutX="10.0" layoutY="416.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="106.0" text="New folder" AnchorPane.bottomAnchor="56.0" />
      <Button fx:id="deleteClient" onMouseClicked="#deleteOnClient" layoutX="129.0" layoutY="414.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="106.0" text="Delete" AnchorPane.bottomAnchor="56.0" AnchorPane.leftAnchor="126.0" />
